{
  "name": "tppm",
  "version": "0.3.20",
  "description": "Trakt Playback Progress Manager",
  "author": "sharkykh",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=11",
    "yarn": ">=1.15"
  },
  "scripts": {
    "start": "yarn build --env.local && node www.js",
    "start:dev": "yarn dev --env.local && node www.js",
    "lint": "eslint --cache **/*.{js,vue}",
    "build": "webpack --mode production --progress",
    "dev": "webpack --mode development --progress",
    "dev-server": "webpack-dev-server --inline --progress",
    "preversion": "yarn && yarn lint && yarn build"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-optional-chaining": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.10.2",
    "@babel/register": "7.10.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.6.5",
    "css-loader": "3.4.2",
    "date-fns": "2.11.0",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.24.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-vue": "6.2.2",
    "express": "4.17.1",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "mini-css-extract-plugin": "0.9.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-vue": "0.10.1",
    "style-loader": "1.1.3",
    "trakt.tv": "sharkykh/trakt.tv-browser#master",
    "vue": "2.6.11",
    "vue-loader": "15.9.0",
    "vue-template-compiler": "2.6.11",
    "vuex": "3.1.3",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-null-plugin": "0.0.2"
  }
}
