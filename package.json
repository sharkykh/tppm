{
  "name": "tppm",
  "version": "0.3.14",
  "description": "Trakt Playback Progress Manager",
  "author": "sharkykh",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=11",
    "yarn": ">=1.15"
  },
  "scripts": {
    "start": "yarn build --env.local && node www.js",
    "start:dev": "yarn dev --env.local && node www.js",
    "lint": "eslint --cache **/*.{js,vue}",
    "build": "webpack --mode production --progress",
    "dev": "webpack --mode development --progress",
    "dev-server": "webpack-dev-server --inline --progress",
    "preversion": "yarn && yarn lint && yarn build"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/preset-env": "7.7.5",
    "@babel/register": "7.7.4",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.4.7",
    "css-loader": "3.2.1",
    "date-fns": "2.8.1",
    "eslint": "6.7.2",
    "eslint-config-xo-space": "0.22.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-vue": "6.0.1",
    "express": "4.17.1",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "4.0.0-beta.5",
    "mini-css-extract-plugin": "0.8.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-vue": "0.8.1",
    "style-loader": "1.0.1",
    "trakt.tv": "https://github.com/sharkykh/trakt.tv-browser#master",
    "vue": "2.6.10",
    "vue-loader": "15.7.2",
    "vue-template-compiler": "2.6.10",
    "vuex": "3.1.2",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-null-plugin": "0.0.2"
  }
}
